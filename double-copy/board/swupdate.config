CONFIG_HAVE_LINUX=y
CONFIG_HAVE_LIBCONFIG=y
CONFIG_HAVE_LIBUBOOTENV=y
CONFIG_HAVE_LIBTEGRABOOT_TOOLS=y
CONFIG_HAVE_LIBBTRFS=y
CONFIG_HAVE_ZLIB=y
CONFIG_HAVE_MBEDTLS=y

#
# SWUpdate Settings
#

#
# General Configuration
#

#
# systemd support needs libsystemd
#
CONFIG_DEFAULT_CONFIG_FILE="/etc/swupdate.cfg"
CONFIG_SCRIPTS=y
# CONFIG_HW_COMPATIBILITY is not set
CONFIG_SW_VERSIONS_FILE="/etc/sw-versions"

#
# Socket Paths
#
CONFIG_SOCKET_CTRL_PATH="/tmp/sockinstctrl"
CONFIG_SOCKET_PROGRESS_PATH="/tmp/swupdateprog"
# end of Socket Paths

#
# MTD support needs libmtd
#

#
# Lua support needs a Lua interpreter
#
# end of General Configuration

#
# Build Options
#
CONFIG_CROSS_COMPILE=""
CONFIG_SYSROOT=""
CONFIG_EXTRA_CFLAGS=""
CONFIG_EXTRA_LDFLAGS=""
CONFIG_EXTRA_LDLIBS=""
# end of Build Options

#
# Debugging Options
#
# CONFIG_DEBUG is not set
# CONFIG_WERROR is not set
# CONFIG_NOCLEANUP is not set
# end of Debugging Options
# end of SWUpdate Settings

#
# Bootloader support
#

#
# Bootloader Interfaces
#
CONFIG_BOOTLOADER_NONE=y

#
# EFI Boot Guard needs libebgenv and libz
#
# CONFIG_UBOOT is not set
# CONFIG_BOOTLOADER_GRUB is not set
# CONFIG_BOOTLOADER_CBOOT is not set
# end of Bootloader Interfaces

CONFIG_BOOTLOADER_DEFAULT_NONE=y
# CONFIG_BOOTLOADER_STATIC_LINKED is not set
CONFIG_UPDATE_STATE_CHOICE_BOOTLOADER=y
CONFIG_UPDATE_STATE_BOOTLOADER="ustate"

#
# Interfaces
#

#
# Image downloading support needs libcurl
#
CONFIG_WEBSERVER=y
CONFIG_MONGOOSE=y
CONFIG_MONGOOSEIPV6=y
# CONFIG_MONGOOSESSL is not set

#
# Security
#
# CONFIG_SSL_IMPL_NONE is not set
CONFIG_SSL_IMPL_MBEDTLS=y
CONFIG_HASH_VERIFY=y
CONFIG_SIGNED_IMAGES=y
CONFIG_SIGALG_RAWRSA=y
# CONFIG_SIGALG_RSAPSS is not set
# CONFIG_SIGALG_GPG is not set
# CONFIG_ENCRYPTED_IMAGES is not set

#
# Compressors (zlib always on)
#
CONFIG_GUNZIP=y

#
# Parsers
#

#
# Parser Features
#
CONFIG_LIBCONFIG=y
CONFIG_PARSERROOT=""
# CONFIG_SETSWDESCRIPTION is not set
# end of Parser Features

#
# Handlers
#

#
# Image Handlers
#

#
# archive support needs libarchive
#
# CONFIG_BOOTLOADERHANDLER is not set
# CONFIG_COPY is not set

#
# diskpart support needs libfdisk
#
# CONFIG_DISKFORMAT_HANDLER is not set
# CONFIG_EMMC_HANDLER is not set
CONFIG_RAW=y

#
# rdiff support needs librsync
#
# CONFIG_READBACKHANDLER is not set

#
# remote handler needs zeromq
#
CONFIG_SHELLSCRIPTHANDLER=y

#
# swuforward handler needs libcurl
#

#
# swuforward handler needs websockets and uriparser
#

#
# ubivol support needs libubi
#

#
# Microcontroller handler depends on libgpiod
#

#
# uniqueuuid support needs libblkid
#
# end of Image Handlers

#
# Containers
#

#
# Update containers
#

#
# Docker support needs libcurl
#
# end of Update containers
